{
    "filter": {
        "media_file_extension": [
            {
                "type": "include",
                "extension": [
                    ".mkv",
                    ".mka",
                    ".ass",
                    ".mp4",
                    ".flac"
                ]
            }
        ],
        "ignore_special_dir": [
            {
                "type": "exclude",
                "filename": [
                    "SPs",
                    "CDs",
                    "Menus",
                    "Glossaries",
                    "Scans",
                    "Previews"
                ]
            }
        ]
    },
    "file": {
        "rename": true,
        "filter": [
            "media_file_extension"
        ]
    },
    "dir": {
        "rename": true,
        "filter": [
            "ignore_special_dir"
        ]
    },
    "move_rule": [
        {
            "description": "Derive series name from media file name",
            "type": "add_parent",
            "file": "file",
            "regex": true,
            "entries": [
                {
                    "source": "\\s*[ABv\\d]+\\.[a-zA-Z\\d\\.]+$",
                    "target": ""
                }
            ]
        }
    ],
    "rename_rule": [
        {
            "description": "Fix dir names",
            "type": "transform",
            "file": "dir",
            "regex": false,
            "entries": [
                {
                    "source": ".mkv",
                    "target": ""
                },
                {
                    "source": ".mp4",
                    "target": ""
                }
            ]
        },
