        {
            "description": "Remove video source",
            "type": "remove",
            "file": "all",
            "regex": true,
            "prefix": "\\s*\\[.*?",
            "suffix": ".*?\\]\\s*",
            "entries": [
                "BiliBili",
                "Baha"
            ]
        },
        {
            "description": "Remove labels in brackets (exact)",
            "type": "remove",
            "file": "all",
            "regex": true,
            "prefix": "\\s*\\[",
            "suffix": "\\]\\s*",
            "entries": [
                "END"
            ]
        },
        {
            "description": "Remove labels in brackets",
            "type": "remove",
            "file": "all",
            "regex": true,
            "prefix": "\\s*\\[",
            "suffix": ".*?\\]\\s*",
            "entries": [
                "(1080)[pP]",
                "(816)[pP]",
                "Ma10[pP]",
                "BDRip",
                "WebRip",
                "BIG5",
                "HEVC",
                "MKV",
                "MP4",
                "GB_MP4",
                "GB",
                "x26[54]",
                "H264",
                "WEB",
                "AVC",
                "CHT",
                "CHS",
                "CHJ_JAP",
                "JPSC",
                "JPTC",
                "SRT",
                "1280X720",
                "1080[pP]_Ma10P",
                "1080[pP]_Hi10P",
                "720[pP]",
                "864[pP]",
                "1920[xX]1080",
                "1920[xX]800",
                "BD[Rr][Ii][Pp][\\-\\s\\w\\d\\_]*",
                "[(Ma)(Hi)]*10p_[(2160)(1080)(720)]*p[\\_\\w]*",
                "x264_[2]*flac",
                "[xX]264_AAC",
                "x265_[2]*flac",
                "x265_ac3",
                "x265_flac_ac3",
                "x265_flac_[2]*aac",
                "AVC_AAC",
                "AVC_FLAC[xX2]*",
                "[0-9A-F]{8}",
                "WEBRIP",
                "Movie[\\w\\(\\)\\.\\s]*",
                "H264_FLAC",
                "HEVC[\\s\\w\\d]*FLAC",
                "HEVC_ALS",
                "BD",
                "B-Global",
                "5\\.1ch",
                "ASSx2",
                "UHDRip",
                "Multiple Subtitle"
            ]
        },
        {
            "description": "Remove labels in parentheses",
            "type": "remove",
            "file": "all",
            "regex": true,
            "prefix": "\\s*\\(.*?",
            "suffix": ".*?\\)\\s*",
            "entries": [
                "BD",
                "DVD",
                "[0-9A-F]{8}",
                "WebRip",
                "1280x720"
            ]
        },
        {
            "description": "Remove other labels",
            "type": "remove",
            "file": "all",
            "regex": true,
            "prefix": "\\s*",
            "suffix": "\\s*",
            "entries": [
                "\\.HKG\\&X2",
                "\\.EMD\\&HKG",
                "\\.HKG",
                "(\\s)*「.*」(\\s)*",
                "\\[資金募集中\\]",
                "\\[[简繁].*?\\]",
                "\\[招.*?\\]",
                "YY-",
                "\\.JYFanSub-",
                "\\ V2",
                "\\[01-12 Fin\\]"
            ]
        },
        {
            "description": "Remove episode brackets",
            "type": "remove",
            "regex": true,
            "file": "file",
            "entries": [
                "\\[(?:\\d\\d)\\]"
            ]
        },
        {
            "description": "Remove final brackets",
            "type": "remove_final_brackets",
            "regex": false,
            "file": "all",
            "entries": []
        },
        {
            "description": "Check renaming results",
            "type": "check",
            "file": "all",
            "regex": true,
            "entries": [
                "\\[.*?[^0-9Ov\\.VAD].*?\\]",
                "\\(.*?[^0-9Ov\\.VAD].*?\\)"
            ]
        },
        {
            "description": "Trans symbols to spaces",
            "type": "transform",
            "file": "all",
            "regex": false,
            "entries": [
                {
                    "source": "‐",
                    "target": " "
                },
                {
                    "source": "[",
                    "target": " "
                },
                {
                    "source": "]",
