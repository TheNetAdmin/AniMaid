                    "target": " "
                },
                {
                    "source": "-",
                    "target": " "
                },
                {
                    "source": "～",
                    "target": " "
                },
                {
                    "source": "~",
                    "target": " "
                },
                {
                    "source": ":",
                    "target": " "
                },
                {
                    "source": ";",
                    "target": " "
                },
                {
                    "source": "：",
                    "target": " "
                },
                {
                    "source": "!",
                    "target": " "
                },
                {
                    "source": "/",
                    "target": " "
                },
                {
                    "source": "／",
                    "target": " "
                },
                {
                    "source": "？",
                    "target": " "
                }
            ]
        },
        {
            "description": "Shrink spaces",
            "type": "transform",
            "file": "all",
            "regex": false,
            "entries": [
                {
                    "source": " .",
                    "target": "."
                },
                {
                    "source": "  ",
                    "target": " "
                }
            ]
        },
        {
            "description": "Strip spaces",
            "type": "remove",
            "file": "all",
            "regex": true,
            "entries": [
                "^\\s+",
                "\\s+$"
            ]
        }
    ]
}
